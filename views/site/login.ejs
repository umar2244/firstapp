<%- include('../header/nav'); %>
<h2> Login</h2>

<form action="/login" , method="post">
    
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" value="ali@gmail.com">
      
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" value="12345">
    </div>
   
    <button type="submit" class="btn btn-primary" >Login</button>
  </form>